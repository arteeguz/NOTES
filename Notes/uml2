    direction LR
    class User {
        -userId: int
        -name: string
        -email: string
        -password: string
        +login()
        +logout()
        +searchDesk()
        +viewOfficeLayout()
        +navigateToDesk()
        +updateProfile()
        +reportIssue()
    }
    class Desk {
        -deskId: int
        -floorNumber: int
        -location: string
        -isAvailable: boolean
        +getDeskInfo()
        +updateAvailability()
    }
    class Office {
        -officeId: int
        -name: string
        -floors: List~Floor~
        +getFloorPlan()
        +updateLayout()
    }
    class Floor {
        -floorNumber: int
        -desks: List~Desk~
        +getDesks()
        +addDesk()
        +removeDesk()
    }
    class NavigationSystem {
        -arEngine: AREngine
        +calculatePath()
        +provideARDirections()
        +updateUserLocation()
    }
    class AuthenticationSystem {
        +authenticateUser()
        +registerUser()
        +resetPassword()
    }
    class Booking {
        -bookingId: int
        -userId: int
        -deskId: int
        -startTime: DateTime
        -endTime: DateTime
        +createBooking()
        +cancelBooking()
    }
    class AREngine {
        -camera: Camera
        -imuSensor: IMUSensor
        +initializeAR()
        +updateAROverlay()
    }
    class AdminDashboard {
        +generateUsageReport()
        +manageUserAccounts()
        +viewSystemLogs()
    }
